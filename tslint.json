{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [true, {"funtions": "never"}],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "no-namespace": false,
        "max-classes-per-file": false,
        "no-empty": false,
        "interface-name": false,
        "max-line-length": [true, 150],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
